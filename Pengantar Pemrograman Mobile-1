void main(){
    // membuat variabel dengan tipe data
    String nama = "Putri Astryani Situmorang";
    int umur = 21;
 
    // mencetak variabel
    print("Nama saya $nama. Umur $umur tahun");
  



    // membuat program kasir seperti perintah c++
    print("### Program Kasir ###");
    print("Total belanja: 250000");
    int totalBelanja = 250000;

    if( totalBelanja >= 100000 ){
        print("Selamat anda dapat hadiah!");
    }
}
